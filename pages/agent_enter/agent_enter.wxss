page{
	background-color: #FFFFFF;
}
.item{
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
	border-bottom: 2rpx solid #DDDDDD;
	padding: 0 20rpx;
	box-sizing: border-box;
    font-size: 26rpx;
	position: relative;
    margin-bottom: 0 !important;
}
.item view{
	z-index: 9;
	color: #333333;
}
.item input{
	z-index: 8;
	position: absolute;
	left: 0;
	top: 0;
	border: 0;
	width: 100%;
	height: 100%;
	outline: none;
	text-align: right;
	padding: 0 20rpx;
	box-sizing: border-box;
}
.spitem{
	width: 100%;
	height: 180rpx;
    font-size: 26rpx;
	line-height: 180rpx;
	position: relative;
	border-bottom: 2rpx solid #DDDDDD;
	background-color: white;
	padding: 0 20rpx;
	box-sizing: border-box;
}
.jjgs,.yrpy{
	width: 140rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	border: 2rpx solid rgb(254,95,155);
	float: right;
	color: rgb(254,95,155) !important;
	border-radius: 6rpx;
	margin-top: 30rpx;
	margin-left: 20rpx;
}
.activesss{
	background-color: rgb(254,95,155) !important;
	color: white !important;
}
.submit{
	width: calc(100% - 40rpx);
	margin-left: 20rpx;
	line-height: 80rpx;
	text-align: center;
	margin-top: 120rpx;
	border-radius: 10rpx;
	color: white;
	background-color: #FE5BA9;
}




.fir{
	width: 126rpx;
	height: 126rpx;
	position: absolute;
	top: 27rpx;
	right: 20rpx;
}