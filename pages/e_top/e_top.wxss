page{
  background: #f2f2f2;
  font-size: 28rpx;
}

.top{
  background: #fff;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
}
.rank{
  border-bottom:1rpx solid #f2f2f2;
padding-bottom:20rpx;
box-sizing: border-box;
}
.rankhead image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}
.rankhead{
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.rankm{
  display: inline-block;
  vertical-align: middle;
}
.heng{
  width: 8rpx;
  height: 34rpx;
  background: #FF5CAB;
  display: inline-block;
  margin-right: 15rpx;
  vertical-align: middle;
}
.jie{
  margin-top: 20rpx;
  display: inline-block;
}
.instr{
  margin-top: 20rpx;
  float: right;
  color: #FF5CAB;
}
.choo{
  display: inline-block;
  margin-left: 26rpx;
  margin-top: 20rpx;
  color: #232323;
  font-size: 26rpx;
}
.li{
  margin-top: 20rpx;
  float: right;
  color: #232323;
  font-size: 26rpx;
}
.pri{
  border-bottom: 1rpx solid #f2f2f2;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.pric{
  display: inline-block;
  width: calc((100% - 80rpx)/3);
  margin-left: 20rpx;
  height: 75rpx;
  background: #E6E6E6;
  line-height: 75rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
}
.inp{
  margin-top: 10rpx;
}
.cole{
  color: #e65099;
}

.agree{
  margin-left: 20rpx;
  margin-top: 120rpx;
}
.pay{
  margin-top: 20rpx;
  width: calc(100% - 40rpx);
  margin-left: 20rpx;
  height: 90rpx;
  background: #FF5CAB;
  text-align: center;
  line-height: 90rpx;
  border-radius: 30rpx;
  color: #fff;
}
.mark{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.4);
    z-index: 12;
}
.marktop{
  width: 100%;
  text-align: center;
  margin-bottom: 20rpx;
}
.markron{
  background: #fff;
  width: calc(100% - 260rpx);
  margin-left: 130rpx;
  margin-top: 200rpx;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  height: 450rpx;
  letter-spacing:1rpx;
}
.close{
  margin-top: 30rpx;
  width: 100%;
  text-align: center;
}
.close image{
  width: 50rpx;
  height: 50rpx;
}
.active{
  background: #FF5CAB;
  color: #fff;
}
.prices{
  display: inline-block;
  color: #FF0000;
  font-size:30rpx;
  margin-left: 20rpx; 
}