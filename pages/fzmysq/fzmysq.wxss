page {
    background-color: #f5f5f5;
}

.item {
    background-color: white;
    width: 100%;
    height: 80px;
    margin-top: 10px;
    padding: 8px 10px;
    box-sizing: border-box;
}

.imgs {
    width: 60px;
    height: 60px;
    border-radius: 60px;
    border: 2px solid #ffcce5;
    position: relative;
    overflow: hidden;
    float: left;
    margin-right: 10px;
}

.imgs image {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.name {
    margin-top: 5px;
    color: #232323;
    font-size: 17px;
    letter-spacing: 1px;
}

.name view {
    float: right;
    color: #ff9705;
}

.reason {
    font-size: 13px;
    color: #999;
    letter-spacing: 1px;
    margin-top: 10px;
}

.reason view {
    float: right;
}


.color {
    color: #f00 !important;
}
.none image{
	width: calc(100% - 126rpx);
	margin-left: 63rpx;
	margin-top: 260rpx;
}
.none view{
	text-align: center;
	letter-spacing: 2rpx;
    font-size: 30rpx;
}
