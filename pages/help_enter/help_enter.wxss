@import '../submit_notice/submit_notice';
.ilebels{
    width: 100%;
    height: 600rpx;
    background-color: white;
    position: fixed;
    bottom: 0;
    left: 0;
}
.tops{
    display: flex;
    justify-content: space-between;
    padding: 0 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
}
.quxiao{
    color: #999999;
    font-size: 32rpx;
}
.sure{
    color: #1AAD19;
    font-size: 32rpx;
}
.ilablein{
    width: 100%;
    height: 520rpx;
    overflow: scroll;
    padding: 20rpx;
    box-sizing: border-box;
    -webkit-overflow-scrolling: touch;
}
.ilablein text{
    display: inline-block;
    width: 200rpx;
    line-height: 60rpx;
    border-radius: 6rpx;
    border: 1px solid #808080;
    text-align: center;
    margin-left: 25rpx;
    margin-bottom: 20rpx;
    color: #808080;
    font-size: 26rpx;
}
.iact{
    color: #fe5ba9 !important;
    border: 1px solid #fe5ba9 !important;
}
.vmask{
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 99rpx;
    background: rgba(0,0,0,0.3);
    text-align: center;
    color: white;
    line-height: 80rpx;
    font-size: 20rpx;
}
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.2);
    z-index: 12;
    /* display: none;  */
}
.maskvideo{
    width: 100%;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 9999;
}
input{
    width: 100%;
    height: 80rpx;
    background-color: #f8f8f8;
    padding: 10rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    font-size: 24rpx;
    letter-spacing: 1rpx;
}