/**index.wxss**/
@import '../../app.wxss';
page{
    background-color: #efefef;
}
.topnav{
    width: 100%;
    line-height: 72rpx;
    background-color: #fe5ba9;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    color: white;
    padding: 14rpx;
    box-sizing: border-box;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
}
.list{
    width: 100%;
    padding: 20rpx 12rpx;
    position: relative;
    top: 92rpx;
    box-sizing: border-box;
}
.poster{
    width: 180rpx;
    height: 166rpx;
    background-color: lightblue;
    position: absolute;
    right: 19rpx;
    bottom: 47rpx;
}
.poster image{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
.iact{
    background-color: #fe5ba9 !important;
    color: white !important;
    border: 1px solid #fe5ba9 !important;
}