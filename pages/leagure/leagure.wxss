page {
    background-color: #f5f5f5;
    padding: 40rpx 20rpx 0 20rpx;
    box-sizing: border-box;
}

.person {
    width: 100%;
    height: 360rpx;
    border-radius: 20rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100% 100%;
    padding: 40rpx 20rpx;
    box-sizing: border-box;
    margin-bottom: 60rpx;
}

.sy {
    display: flex;
    justify-content: space-between;
    margin-top: 40rpx;
    color: white;
    text-align: center;
    font-size: 24rpx;
}

.sy>view {
    width: 50%;
}

.sy>view:first-child {
    border-right: 2rpx solid #fff;
}

.headimg {
    width: 120rpx;
    height: 120rpx;
    border-radius: 120rpx;
    border: 4rpx solid white;
    position: relative;
    overflow: hidden;
    float: left;
    margin-right: 18rpx;
}

.headimg image {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.pname {
    font-size: 34rpx;
    color: white;
    letter-spacing: 2rpx;
    margin-top: 6rpx;
}

.pc {
    font-size: 26rpx;
    color: white;
    letter-spacing: 2rpx;
    margin-top: 20rpx;
}

.pname view {
    display: inline-block;
    width: 70rpx;
    line-height: 38rpx;
    font-size: 24rpx;
    color: #ff1a34;
    text-align: center;
    background-color: white;
    border-radius: 38rpx;
}

.jointime {
    font-size: 26rpx;
    color: white;
    letter-spacing: 2rpx;
    margin-top: 30rpx;
    padding-left: 144rpx;
    box-sizing: border-box;
}

.litem {
    width: 100%;
    height: 130rpx;
    background-color: white;
    border-radius: 20rpx;
    margin-bottom: 20rpx;
    padding: 25rpx 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #272b34;
}

.ico {
    width: 80rpx;
    height: 80rpx;
    float: left;
    margin-right: 20rpx;
}

.enter {
    width: 14rpx;
    height: 24rpx;
    position: relative;
    float: right;
    margin-left: 10rpx;
    top: 28rpx;
}

.litem view {
    font-size: 26rpx;
    color: #999;
    float: right;
}
