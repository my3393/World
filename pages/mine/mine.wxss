/* pages/mine/mine.wxss */
page{
    background: #f2f2f2;
}
.top{
	width: 100%;
	height: 360rpx;
	background: url(data:image/png;base64,) no-repeat center #FE5BA9;
	background-size: 100%;
	position: relative;
}
.uname{
	font-size: 34rpx;
	color: #FFFFFF;
	position: relative;
	top: 60rpx;
	padding-left: 24rpx;
	box-sizing: border-box;
}
.uid{
	font-size: 26rpx;
	color: #FFFFFF;
	padding-left: 24rpx;
	box-sizing: border-box;
	position: relative;
	top: 80rpx;
}
.tj{
	display: flex;
	justify-content: space-between;
	position: absolute;
	bottom: 30rpx;
	padding-left: 24rpx;
}
.tj view{
	text-align: center;
	color: white;
	font-size: 28rpx;
	margin-right: 60rpx;
}
.ruzhu{
	color: #FE5BA9;
	font-size: 28rpx;
	text-align: center;
	width: 140rpx;
	line-height: 60rpx;
	border-radius: 6rpx;
	background-color: white;
	position: absolute;
	bottom: 30rpx;
	right: 40rpx;
}
.mitem{
	line-height: 120rpx;
	padding-left: 24rpx;
	box-sizing: border-box;
	border-bottom: 2rpx solid #DDDDDD;
}
.mitem view{
    display: inline-block;
}
.mitem image{
	width: 38rpx;
    height: 38rpx;
	vertical-align: middle;
	position: relative;
	top: -2rpx;
	margin-right: 10rpx;
}
.mitem{
	font-size: 28rpx;
	background-color: white;
	color: #333333;
}
.allrm{
	float: right;
}
.allrm img{
	width: 12rpx;
	vertical-align: middle;
	margin-left: 20rpx;
}
.wrap{
	width: 100%;
	top: 0;
	overflow: scroll;
}
.lables{
	position: relative;
	top: 100rpx;
	color: white;
	padding-left: 24rpx;
    font-size: 24rpx;
	box-sizing: border-box;
	
}
.pimg{
	width: 140rpx;
	height: 140rpx;
	border-radius: 140rpx;
	border: 4rpx solid #f94b62;
	box-sizing: border-box;
	float: right;
	margin-top: 40rpx;
	margin-right: 40rpx;
	overflow: hidden;
	position: relative;
}
.pimg image{
	height: 100%;
    width: 100%;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
.allrm image{
    width: 16rpx !important;
    height: 26rpx;
    position: relative;
    top: -2rpx;
}