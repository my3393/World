page{
	background-color: #f5f5f5;
}
.apply_tip{
	font-size: 26rpx;
	color: #999999;
	padding: 0 20rpx;
	box-sizing: border-box;
	line-height: 88rpx;
	letter-spacing: 2rpx;
}
.aitem{
	line-height: 100rpx;
	width: 100%;
	padding: 0 20rpx;
	box-sizing: border-box;
	background-color: white;
	border-bottom: 2rpx solid #e6e6e6;
    position: relative;
}
.aitem view{
	display: inline-block;
	width: 100rpx;
	font-size: 30rpx;
	letter-spacing: 2rpx;
	color: #232323;
}
.aitem input{
	width: 100%;
	float: right;
	outline: none;
	height: 100rpx;
    font-size: 26rpx;
	border: 0;
	background: transparent;
	text-align: right;
    position: absolute;
    top: 0;
    right: 20rpx;
    padding: 0;
}
.uptitle{
	background-color: white;
	color: #232323;
	font-size: 30rpx;
	line-height: 88rpx;
	padding: 0 20rpx;
	box-sizing: border-box;
	letter-spacing: 2rpx;
}
.upfiles{
	width: 100%;
	display: flex;
	background-color: white;
	padding: 20rpx 20rpx;
	padding-top: 4rpx;
	box-sizing: border-box;
	justify-content: space-between;
}
.upfiles view{
	width: 48%;
	text-align: center;
	font-size: 26rpx;
	color: #999999;
}
.upfiles view image{
    width: 100%;
	height: 230rpx;
}
.abottom{
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	padding: 20rpx;
	box-sizing: border-box;
}
.aptip{
	text-align: center;
	color: #999;
	letter-spacing: 2rpx;
	font-size: 26rpx;
	margin-bottom: 20rpx;
}
.aptip view{
	color: #FF5CAB;
}
.submit{
	width: 100%;
	line-height: 100rpx;
	background-color: #FF5CAB;
	color: white;
	text-align: center;
	border-radius: 100rpx;
	letter-spacing: 2rpx;
}
.searchs{
	width: 100%;
	height: 100rpx;
	padding: 20rpx;
	box-sizing: border-box;
	background-color: #FF5CAB;
}
.searchs input{
	width: calc(100% - 100rpx);
	height: 60rpx;
    background-color: white;
	border: 0;
	border-radius: 60rpx;
	outline: none;
    float: left;
	padding: 0 20rpx;
    font-size: 30rpx;
	box-sizing: border-box;
}
.searchs view{
    float: right;
    font-size: 30rpx;
	color: white;
	margin-left: 20rpx;
    position: relative;
    top: 10rpx;
}

#clipArea {
	height: calc(100% - 120rpx);
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
}

#clipBtn {
	width: 50%;
	line-height: 120rpx;
	position: fixed;
	bottom: 0;
	right: 0;
	background-color: #FE5BA9;
	color: white;
	letter-spacing: 2rpx;
	font-size: 28rpx;
	text-align: center;
}
#file {
	width: 50%;
	line-height: 120rpx;
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: #FE5BA9;
	color: white;
	letter-spacing: 2rpx;
	font-size: 28rpx;
	z-index: 999;
	opacity: 0;
	text-align: center;
}
.clip{
	display: none;
}
.chs{
	width: 50%;
	line-height: 120rpx;
	position: fixed;
	bottom: 0;
	left: 0;
	opacity: 2;
	background-color: #ffffff;
	color: #333333;
	letter-spacing: 2rpx;
	font-size: 28rpx;
	text-align: center;
}
.loading{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,.3);
	position: fixed;
	top: 0;
	display: none;
	left: 0;
}
.loading image{
	width: 200rpx;
	height: 200rpx;
	margin-top: 50%;
	margin-left: calc(50% - 100rpx);

}
.loads{
	letter-spacing: 2rpx;
	font-size: 28rpx;
	color: #fff;
	text-align: center;
}