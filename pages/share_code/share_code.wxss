page{
	background: url(data:image/png;base64,) no-repeat #FAFAFA;
	background-size: 100% auto;
}
.wrap{
	width: calc(100% - 40rpx);
	height: 900rpx;
	border-radius: 20rpx;
	background-color: #fff5fa;
	margin-left: 20rpx;
	position: relative;
	top: 80rpx;
	padding: 30rpx;
	box-sizing: border-box;box-shadow: 0 0 16rpx #b5aeae;
}
.himg{
	width: 120rpx;
	height: 120rpx;
	position: relative;
	overflow: hidden;
	border-radius: 120rpx;
	border: 4rpx solid #FFCCE5;
	float: left;
	margin-right: 20rpx;
}
.himg image{
	height: 100%;
    width: 100%;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
.name{
	font-size: 34rpx;
	color: #232323;
	letter-spacing: 2rpx;
	margin-top: 8rpx;
}
.labelss{
	font-size: 26rpx;
	color: #999999;
	margin-top: 20rpx;
	letter-spacing: 2rpx;
}
.codes{
	width: 512rpx;
	height: 512rpx;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100% 100%;
	margin-left: calc(50% - 256rpx);
	margin-top: 100rpx;
	position: relative;
}
.codes image{
	width: 75%;
	height: 75%;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
.tipssss{
	font-size: 30rpx;
	color: #ff5cab;
	margin-top: 40rpx;
	letter-spacing: 2rpx;
	text-align: center;
}
.save{
	display: inline-block;
	margin-left: 20rpx;
	width: calc(100% - 40rpx);
	background-color: #ff5cab;
	color: white;
	line-height: 90rpx;
	border-radius: 90rpx;
	text-align: center;
	position: relative;
	top: 200rpx;
}
.cun{
	width: calc(100% - 60rpx);
	margin-left: 30rpx;
	margin-top: 100rpx;
	background: #ff5cab;
	text-align: center;
	height: 70rpx;
	line-height: 70rpx;
	color: #fff;
	border-radius: 32rpx;
	font-size: 30rpx;
}
