/* pages/share_member/share_member.wxss */
page{
  font-size: 30rpx;
  background: #f2f2f2;
}
.top{
  width: 100%;
  height: 60rpx;
  background: #FF657E;
  padding-left: 50rpx;
  box-sizing: border-box;
  line-height: 60rpx;
  color: #fff;
}
.nav{
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1rpx solid #ddd;
}
.nav-head{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
  vertical-align: middle;
  display: inline-block;
  margin-right: 30rpx;
}
.nav-head image{
  width: 100%;
  height: 100%;
}
.nav-name{
  display: inline-block;
  vertical-align: middle;
}
