/* pages/wallet_succ/wallet_succ.wxss */
page{
  font-size: 30rpx;

}
.item{
  position: absolute;
  top: 20%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.item-img{
  width: 98rpx;
  height: 98rpx;
  display: inline-block;

}
.item-img image{
  width: 100%;
  height: 100%;
}
.ref{
  margin: 12rpx 0;

}
.back{
  margin-top: 24rpx;
  width: 200rpx;
  font-size: 28rpx;
  display: inline-block;
  border-radius: 8rpx;
  text-align: center;
  padding: 5rpx 15rpx;
  box-sizing: border-box;
  border: 1rpx solid #FD4C69;
  color: #FD4C69;
}