/* pages/wallet_withdrawal/wallet_withdrawal.wxss */
page{
  font-size: 30rpx;
}
.top{
  padding:0 20rpx;
  box-sizing: border-box;
  width: 100%;
  height: 119rpx;
  background: #F0F0F0;
  line-height: 119rpx;
  padding-left: 30rpx;
  color: #808080;
}
.tix{
  width: 42rpx;
  height: 42rpx;
  display: inline-block;
  margin-right: 15rpx;
}
.tix image{
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
.tis{
  display: inline-block;
  vertical-align: middle;
  color: #808080;
}
.tis input{
  width: 440rpx;
}
.hr{
  margin: 20rpx;
}
.banlan{
  
  color: #333333;
}
.all{
  float: right;
  color: #FE5BA9;
}

.nav{
  padding: 20rpx;
  box-sizing: border-box;
 
  border-bottom: 1rpx solid #F0F0F0;
}
.submit{
  margin-top: 60rpx;
  background: linear-gradient(#FE5BA9,#FF6977);
  width: calc(100% - 40rpx);
  margin-left: 20rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  border-radius: 8rpx;
}
.list{
  margin-left: 20rpx;
  color: #CCCCCC;
  margin-top: 40rpx;
}