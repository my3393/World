page {
    padding: 20rpx;
    box-sizing: border-box;
}

page {
    background-color: #f5f5f5;
}

.item {
    width: 100%;
    height: 160rpx;
    border-radius: 20rpx;
    background-color: white;
    padding: 20rpx;
    box-sizing: border-box;
    position: relative;
    margin-bottom: 20rpx;
}

.imgs {
    width: 112rpx;
    height: 112rpx;
    position: relative;
    overflow: hidden;
    border-radius: 112rpx;
    border: 4rpx solid #ffcce5;
    float: left;
    margin-right: 20rpx;
}

.imgs image {
    height: 100%;
    width: 100%;
    position: relative;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.name {
    font-size: 34rpx;
    color: #232323;
    margin-top: 10rpx;
    letter-spacing: 2rpx;
    font-weight: 400;
}

.shouyi {
    font-size: 26rpx;
    color: #999;
    letter-spacing: 2rpx;
    margin-top: 14rpx;
}

.remove {
    width: 120rpx;
    line-height: 50rpx;
    color: #ff5cab;
    border: 2rpx solid #ff5cab;
    position: absolute;
    top: 54rpx;
    right: 20rpx;
    text-align: center;
    font-size: 28rpx;
    border-radius: 10rpx;
}

.spopups {
    width: 560rpx;
    height: 400rpx;
    border-radius: 10rpx;
    background-color: white;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.sptip {
    font-size: 36rpx;
    letter-spacing: 2rpx;
    color: #232323;
    margin-top: 180rpx;
    text-align: center;
}

.spopups image {
    width: 184rpx;
    height: 184rpx;
    position: absolute;
    top: -92rpx;
    left: calc(50% - 92rpx);
}

.shother {
    width: 100%;
    display: flex;
    justify-content: space-between;
    position: fixed;
    bottom: 0;
    left: 0;
    border-bottom-right-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
    overflow: hidden;
}

.shother view {
    width: 50%;
    line-height: 100rpx;
}

.shother view:first-child {
    background-color: #ffcce5;
    color: #ff5cab;
    font-size: 36rpx;
    text-align: center;
}

.shother view:last-child {
    background-color: #ff5cab;
    color: #fff;
    font-size: 36rpx;
    text-align: center;
}
.none image{
	width: calc(100% - 126rpx);
	margin-left: 63rpx;
	margin-top: 260rpx;
}
.none view{
	text-align: center;
	letter-spacing: 2rpx;
    font-size: 30rpx;
}
